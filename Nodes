//Class Node
public class Nodes {
  // properties of Node
  public int data;
  private Node prev;
  private Node next;

  // Constructors of Node
  Nodes(int d) { 
    this.data = d; 
    this.next = null;
    this.prev = null;
  }
  
  // getters and setters
  public int getData(){
    return this.data;
  }
  
  public Node getPrev(){
    return this.prev;
  }
  
  public Node getNext() {
    return this.next;
  }
  
  public void setData(int newData) {
    this.data = newData;
  }
  
  public void setPrev(Node newPrev) {
    this.prev = newPrev;
  }
  
  public void setNext(Node newNext) {
    this.next = newNext;
  }
  
  // Methods
  public void print() {
    //print data
    System.out.print(data + " " );
  }
}
